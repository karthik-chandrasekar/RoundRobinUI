<h1>Editing Config Parameter</h1>
 
 <%= form_for :article, url: article_path(@article), method: :patch do |f| %>

   <br/> 
   
   <%= radio_button_tag 'country', 'UnitedStates', @us %>
   <%= label_tag 'UnitedStates'%>

   <%= radio_button_tag 'country', 'India', @india %>
   <%= label_tag 'India'%>

   <%= radio_button_tag 'country', 'China', @china %>
   <%= label_tag 'China'%>
     
   <br/>
   <%= radio_button_tag 'resourcename', 'sscaria', @sscaria %>
   <%= label_tag 'Subash Scaria' %>

   <%= radio_button_tag 'resourcename', 'nimathur', @nimathur %>
   <%= label_tag 'Nitin Mathur' %>

   <br/>
   <%= check_box_tag 'hadoop', 'hadoop', @hadoop %> 
   <%= label_tag 'Hadoop' %>

   <%= check_box_tag 'teradata', 'teradata', @teradata %>
   <%= label_tag 'Teradata' %>
 
   <p>
    <%=f.submit "Save" %>
   </p>   
   
 <%end%>   
 <%= link_to 'Back', articles_path %>
