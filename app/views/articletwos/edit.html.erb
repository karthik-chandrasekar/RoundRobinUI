<h1>Editing Generic Config Parameter</h1>
 
 <%= form_for :articletwo, url: articletwo_path(@articletwo), method: :patch do |f| %>
   <p>
    <%= f.label :title %><br>
    <%= f.text_area :key %>
   </p>
                                                      
   <p>
    <%= f.label :text %><br>
    <%= f.text_area :value %>
   </p>
 
   <p>
    <%=f.submit "Save" %>
   </p>   
   
 <%end%>   
 <%= link_to 'Back', articletwos_path %>
