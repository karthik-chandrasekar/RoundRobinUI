<h1>Add  New Config Parameter</h1>

<%= form_for :articletwos, url: articletwos_path do |f| %>
  <p>
      <%= f.label :title %><br>
      <%= f.text_area :title %>
  </p>
             
  <p>
      <%= f.label :text %><br>
      <%= f.text_area :text %>
  </p>
                          
  <p>
      <%= f.submit :value => "Add Parameter" %>
  </p>
<% end %>

<%= link_to 'Back', articletwos_path%>
