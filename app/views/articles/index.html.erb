<h1 align="Center">Resource-Category Ticketallocator  Parameters</h1>
 
<table>
   <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="2"></th>
   </tr>
              
   <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.text %></td>
      <td><%= link_to 'Show', article_path(article)%></td>
      <td><%= link_to 'Edit', edit_article_path(article)%></td>
      <td><%= link_to 'Delete', article_path(article),method: :delete, data: {confirm: 'Are you sure?'} %></td>  
    </tr>
   <% end %>
</table>

<br/><%= link_to 'View Generic Parameters', articletwos_path %>
