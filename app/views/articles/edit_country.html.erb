<h1>Editing country</h1>

<%= form_for :articles, url: articles_update_country_path(@country), method: :get do |f| %>
    <p>
        <%= cname = @country.countryname%>
        <%= f.label :country %><br>
        <%= f.text_area :cname %>
    </p>
    <p>
        <%= f.submit "Save" %>
    </p>
<%end%>
