<h1 align ="center">Resources</h1>

<%= form_for :articles, method: "get", url: articles_add_resource_path do |f| %>
    <p>
        <%= f.label :title %><br>
        <%= f.text_area :title %>
    </p>

    <p>
        <%= f.submit :value => 'Add Resource' %>
    </p>

<% end %>

<%= link_to 'Back', new_article_path %>
<%= link_to 'Resource List', articles_resource_list_path %>

